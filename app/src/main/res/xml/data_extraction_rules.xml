<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Disable cloud backup for privacy -->
        <exclude domain="datastore" />
        <exclude domain="database" />
        <exclude domain="shared_pref" />
        <exclude domain="external" />
    </cloud-backup>
    
    <device-transfer>
        <!-- Disable device transfer to prevent data leaks -->
        <exclude domain="datastore" />
        <exclude domain="database" />
        <exclude domain="shared_pref" />
        <exclude domain="external" />
    </device-transfer>
</data-extraction-rules>
