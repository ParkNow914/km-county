# Configurações do Firebase
FIREBASE_API_KEY=your_firebase_api_key_here
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_APP_ID=1:123456789012:android:abcdef1234567890abcdef
FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
FIREBASE_MESSAGING_SENDER_ID=123456789012
FIREBASE_MEASUREMENT_ID=G-ABCDEFGHIJ

# Configurações da API (se houver)
API_BASE_URL=https://api.kmcounty.com
API_KEY=your_api_key_here

# Configurações de depuração
DEBUG=true
LOG_LEVEL=DEBUG

# Configurações de build
ENABLE_CRASHLYTICS=true
ENABLE_ANALYTICS=true

# Chaves de API de terceiros (ex: Google Maps, etc.)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Configurações de ambiente
ENVIRONMENT=development
